{"ast":null,"code":"import { filtrarProdutos } from '@/services/ComprarService.js';\nimport { ElTable, ElTableColumn } from 'element-plus';\nexport default {\n  name: 'TabelaProdutos',\n  mounted() {\n    this.filtrarProdutosView();\n  },\n  components: {\n    ElTable,\n    ElTableColumn\n  },\n  data() {\n    return {\n      produtos: [],\n      paginacao: {\n        pagina: 0,\n        ordenacao: 'id',\n        tamanho: 10,\n        total: 0\n      }\n    };\n  },\n  methods: {\n    async filtrarProdutosView() {\n      try {\n        const response = await filtrarProdutos({\n          pagina: this.paginacao.pagina + 1,\n          ordenacao: this.paginacao.ordenacao,\n          tamanho: this.paginacao.tamanho\n        });\n        console.log('Resposta do serviço:', response); // Verifica a resposta completa\n\n        if (response && response.data && response.data.itens && response.data.paginacao) {\n          this.produtos = response.data.itens;\n          console.log('Produtos:', this.produtos); // Verifica se os produtos foram atribuídos corretamente\n          this.paginacao.total = response.data.paginacao.total;\n        } else {\n          console.error('Resposta do serviço não possui dados esperados.');\n        }\n      } catch (error) {\n        console.error('Erro ao filtrar produtos:', error);\n      }\n    },\n    atualizarPagina(pagina) {\n      this.paginacao.pagina = pagina;\n      this.filtrarProdutos();\n    },\n    mudarPagina(novaPagina) {\n      this.paginacao.pagina = novaPagina;\n      this.params.page = novaPagina;\n      this.filtrarProdutos();\n    }\n  }\n};","map":{"version":3,"mappings":"AAUA,SAAQA,eAAe,QAAO,8BAA8B;AAC5D,SAASC,OAAO,EAAEC,aAAY,QAAS,cAAc;AAErD,eAAe;EACXC,IAAI,EAAE,gBAAgB;EACtBC,OAAOA,GAAG;IACN,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC9B,CAAC;EACDC,UAAU,EAAE;IACRL,OAAO;IACPC;EACJ,CAAC;EACDK,IAAIA,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE;QACCC,MAAM,EAAE,CAAC;QACTC,SAAS,EAAE,IAAI;QACfC,OAAO,EAAE,EAAE;QACXC,KAAK,EAAE;MACX;IA