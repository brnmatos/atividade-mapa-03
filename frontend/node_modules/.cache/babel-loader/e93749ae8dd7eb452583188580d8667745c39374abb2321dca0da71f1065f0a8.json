r\n\r\n                console.log(response);\r\n                //if (Array.isArray(response.data.itens)) {\r\n                    //const jsonString = JSON.stringify(response.data.itens);\r\n                    //this.produto = JSON.parse(jsonString);\r\n                this.produto = response.data.itens;\r\n\r\n                console.log('Produtos:', this.produto);\r\n\r\n                this.paginacao.total = response.data.paginacao.total;\r\n        },\r\n\r\n        async novoMetodoFiltrarProdutosView(){\r\n\r\n            await filtrarProdutos({\r\n                    pagina: this.paginacao.pagina + 1,\r\n                    ordenacao: this.paginacao.ordenacao,\r\n                    tamanho: this.paginacao.tamanho\r\n                })\r\n                .then(({ data }) => {\r\n                    this.$notify.success({\r\n                        message: data,\r\n                        duration: 10000\r\n                    });\r\n                    this.fechar();\r\n                    this.$emit('atualizar-restricao-inibido');\r\n                })\r\n                .catch(() => {\r\n                    this.$notify.error({\r\n                        message:\r\n                            'Não foi possível inibir a restrição. ' +\r\n                            'Se o problema persistir, entre em contato com suporte.'\r\n                    });\r\n                    this.fechar();\r\n                })\r\n                .finally(() => {\r\n                    this.payload = {\r\n                        codigoCamada: null,\r\n                        codigoRestricao: null,\r\n                        idsOperacoes: [],\r\n                        idJustificativa: null,\r\n                        descricaoJustificativa: null,\r\n                        acao: 'INSERIR'\r\n                    };\r\n                });\r\n\r\n\r\n        },\r\n\r\n        atualizarPagina(pagina) {\r\n            this.paginacao.pagina = pagina;\r\n            this.filtrarProdutos();\r\n        },\r\n\r\n        mudarPagina(novaPagina) {\r\n            this.paginacao.pagina = novaPagina;\r\n            this.params.page = novaPagina;\r\n            this.filtrarProdutos();\r\n        }\r\n\r\n\r\n\r\n    }\r\n};\r\n</script>\r\n<style lang=\"scss\">\r\n#tabela-gestao-contrato-originacao {\r\n\r\n    .el-table {\r\n        border-top-right-radius: 11px;\r\n        border-top-left-radius: 11px;\r\n    }\r\n    .status-processamento {\r\n        max-width: 148px;\r\n        word-break: break-word;\r\n        font-size: 13px;\r\n        line-height: 17px;\r\n    }\r\n    .tabela-gestao-contrato-originacao__paginacao {\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        background: var(--white);\r\n        padding: 14px 11px;\r\n        border-bottom-left-radius: 11px;\r\n        border-bottom-right-radius: 11px;\r\n\r\n        .pagina {\r\n            color: var(--primary);\r\n            font-weight: 600;\r\n        }\r\n    }\r\n    .tabela-gestao-icone {\r\n        i {\r\n            font-size: 27px;\r\n        }\r\n    }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}