{"ast":null,"code":"import { filtrarProdutos } from '@/services/ComprarService.js';\nimport { ElTable, ElTableColumn } from 'element-plus';\nexport default {\n  name: 'TabelaProdutos',\n  mounted() {\n    this.filtrarProdutosView();\n  },\n  components: {\n    ElTable,\n    ElTableColumn\n  },\n  data() {\n    return {\n      produtos: [],\n      paginacao: {\n        pagina: 0,\n        ordenacao: 'id',\n        tamanho: 10,\n        total: 0\n      }\n    };\n  },\n  methods: {\n    async filtrarProdutosView() {\n      await filtrarProdutos({\n        pagina: this.paginacao.pagina + 1,\n        ordenacao: this.paginacao.ordenacao,\n        tamanho: this.paginacao.tamanho\n      });\n      console.log(response);\n      //if (Array.isArray(response.data.itens)) {\n      //const jsonString = JSON.stringify(response.data.itens);\n      //this.produto = JSON.parse(jsonString);\n      this.produto = response.data.itens;\n      console.log('Produtos:', this.produto);\n      this.paginacao.total = response.data.paginacao.total;\n    },\n    async novoMetodoFiltrarProdutosView() {\n      await filtrarProdutos({\n        pagina: this.paginacao.pagina + 1,\n        ordenacao: this.paginacao.ordenacao,\n        tamanho: this.paginacao.tamanho\n      }).then(({\n        data\n      }) => {\n        this.$notify.success({\n          message: data,\n          duration: 10000\n        });\n        this.fechar();\n        this.$emit('atualizar-restricao-inibido');\n      }).catch(() => {\n        this.$notify.error({\n          message: 'Não foi possível inibir a restrição. ' + 'Se o problema persistir, entre em contato com suporte.'\n        });\n        this.fechar();\n      }).finally(() => {\n        this.payload = {\n          codigoCamada: null,\n          codigoRestricao: null,\n          idsOperacoes: [],\n          idJustificativa: null,\n          descricaoJustificativa: null,\n          acao: 'INSERIR'\n        };\n      });\n    },\n    atualizarPagina(pagina) {\n      this.paginacao.pagina = pagina;\n      this.filtrarProdutos();\n    },\n    mudarPagina(novaPagina) {\n      this.paginacao.pagina = novaPagina;\n      this.params.page = novaPagina;\n      this.filtrarProdutos();\n    }\n  }\n};","map":{"version":3,"mappings":"AAUA,SAAQA,eAAe,QAAO,8BAA8B;AAC5D,SAASC,OAAO,EAAEC,aAAY,QAAS,cAAc;AAErD,eAAe;EACXC,IAAI,EAAE,gBAAgB;EACtBC,OAAOA,GAAG;IACN,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC9B,CAAC;EACDC,UAAU,EAAE;IACRL,OAAO;IACPC;EACJ,CAAC;EACDK,IAAIA,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE;QACCC,MAAM,EAAE,CAAC;QACTC,SAAS,EAAE,IAAI;QACfC,OAAO,EAAE,EAAE;QACXC,KAAK,EAAE;MACX;IACR;EAER,CAAC;EACDC,OAAO,EAAE;IAEL,MAAMT,mBAAmBA,GAAG;MAExB,MAAML,eAAe,CAAC;QACdU,MAAM,EAAE,IAAI,CAACD,SAAS,CAACC,MAAK,GAAI,CAAC;QACjCC,SAAS,EAAE,IAAI,CAACF,SAAS,CAACE,SAAS;QACnCC,OAAO,EAAE,IAAI,CAACH,SAAS,CAACG;MAC5B,CAAC,CAAC;MAEFG,OAAO,CAACC,GAAG,CAACC,QAAQ,CAAC;MACrB;MACI;MACA;MACJ,IAAI,CAACC,OAAM,GAAID,QAAQ,CAACV,IAAI,CAACY,KAAK;MAElCJ,OAAO,CAACC,GAAG,CAAC,WAAW,EAAE,IAAI,CAACE,OAAO,CAAC;MAEtC,IAAI,CAACT,SAAS,CAACI,KAAI,GAAII,QAAQ,CAACV,IAAI,CAACE,SAAS,CAACI,KAAK;IAC5D,CAAC;IAED,MAAMO,6BAA6BA,GAAE;MAEjC,MAAMpB,eAAe,CAAC;QACdU,MAAM,EAAE,IAAI,CAACD,SAAS,CAACC,MAAK,GAAI,CAAC;QACjCC,SAAS,EAAE,IAAI,CAACF,SAAS,CAACE,SAAS;QACnCC,OAAO,EAAE,IAAI,CAACH,SAAS,CAACG;MAC5B,CAAC,EACAS,IAAI,CAAC,CAAC;QAAEd;MAAK,CAAC,KAAK;QAChB,IAAI,CAACe,OAAO,CAACC,OAAO,CAAC;UACjBC,OAAO,EAAEjB,IAAI;UACbkB,QAAQ,EAAE;QACd,CAAC,CAAC;QACF,IAAI,CAACC,MAAM,CAAC,CAAC;QACb,IAAI,CAACC,KAAK,CAAC,6BAA6B,CAAC;MAC7C,CAAC,EACAC,KAAK,CAAC,MAAM;QACT,IAAI,CAACN,OAAO,CAACO,KAAK,CAAC;UACfL,OAAO,EACH,uCAAsC,GACtC;QACR,CAAC,CAAC;QACF,IAAI,CAACE,MAAM,CAAC,CAAC;MACjB,CAAC,EACAI,OAAO,CAAC,MAAM;QACX,IAAI,CAACC,OAAM,GAAI;UACXC,YAAY,EAAE,IAAI;UAClBC,eAAe,EAAE,IAAI;UACrBC,YAAY,EAAE,EAAE;UAChBC,eAAe,EAAE,IAAI;UACrBC,sBAAsB,EAAE,IAAI;UAC5BC,IAAI,EAAE;QACV,CAAC;MACL,CAAC,CAAC;IAGV,CAAC;IAEDC,eAAeA,CAAC5B,MAAM,EAAE;MACpB,IAAI,CAACD,SAAS,CAACC,MAAK,GAAIA,MAAM;MAC9B,IAAI,CAACV,eAAe,CAAC,CAAC;IAC1B,CAAC;IAEDuC,WAAWA,CAACC,U