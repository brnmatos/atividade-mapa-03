alMusica: 0,\r\n      primeiraExecucao: true,\r\n    }\r\n  },\r\n\r\n  methods: {\r\n\r\n    \r\n    // Something to do before playing\r\n    handleBeforePlay(next) {\r\n      console.log('enter handle before...')\r\n      this.buscarNomeMusica()\r\n      this.ouvirMusicaPrimeiraExecucao()\r\n\r\n      next()\r\n    },\r\n\r\n    pauseMethod(option){\r\n      this.tempoInicialMusica = this.$refs.audioPlayer.currentTime;\r\n      this.audioList = [];\r\n      this.buscarNomeMusica();\r\n      this.audioList.push({\r\n        'name': this.currentAudioName, \r\n        'url': 'http://localhost:9090/musica/ouvir'\r\n      });\r\n    },\r\n\r\n\r\n    buscarNomeMusica(){\r\n      buscarNomeMusica().then((resposta) => {\r\n        this.currentAudioName = resposta;\r\n      });      \r\n    },\r\n    \r\n    getMusicas() {            \r\n      ouvirMusica().then((resposta) => {\r\n        this.audioList.push({\r\n          'url': resposta\r\n        }); \r\n      });              \r\n    },\r\n    \r\n    ouvirMusicaPrimeiraExecucao(){\r\n\r\n      if(this.primeiraExecucao){\r\n        console.log('primeira exec');\r\n        console.log(this.currentAudioName);\r\n        this.audioList.push({\r\n          'name': this.currentAudioName, \r\n          'url': 'http://localhost:9090/musica/ouvir'\r\n        });\r\n        this.primeiraExecucao = !this.primeiraExecucao;\r\n\r\n      }else{\r\n\r\n        console.log('segunda ou mais exec');\r\n        this.audioList.push({\r\n          'name': 'intervalo.mp3', \r\n          'url': 'http://localhost:9090/musica/intervalo'\r\n        });\r\n        this.primeiraExecucao = !this.primeiraExecucao;\r\n      }\r\n    },\r\n  },\r\n\r\n  created() {\r\n    //this.buscarNomeMusica();   \r\n    //this.ouvirMusicaPrimeiraExecucao();\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  #nav {\r\n    background-color: #222;\r\n    border-bottom: 4px solid #111;\r\n    padding: 15px 50px;\r\n    display: flex;\r\n    justify-content: f