{"ast":null,"code":"import { anunciar, enviarImagem } from '@/services/AnunciarService.js';\nexport default {\n  name: 'CadastrarProdutoView',\n  components: {},\n  data() {\n    return {\n      nomeProduto: null,\n      valorProduto: null,\n      statusEmpresa: [],\n      totalContratosOriginacao: {\n        totalContratosOriginacao: null,\n        totalImoveis: null,\n        totalVolumeEstimado: null,\n        totalVolumeEntregue: null\n      },\n      contratosTabela: [],\n      paginacao: {},\n      params: {\n        page: 0,\n        size: 20,\n        sort: 'statusSubEmpresa',\n        por: 'desc'\n      },\n      filtro: {\n        numeroContrato: '',\n        idsCultura: [],\n        idsSafra: [],\n        codigosStatus: [],\n        dataContrato: ''\n      },\n      skeletonBlocoInforDados: true,\n      CheckListagemContratoErro: false,\n      selectedFile: null,\n      imageUrl: null\n    };\n  },\n  methods: {\n    cadastrar() {\n      const modal = {\n        \"nomeProduto\": this.nomeProduto,\n        \"valorProduto\": this.valorProduto\n      };\n      anunciar({\n        nomeProduto: this.nomeProduto,\n        valorProduto: this.valorProduto\n      }).then(response => {\n        console.log('Resposta do backend:', response);\n        const produto = response;\n        this.enviarImagemParaBackend(this.selectedFile, produto);\n      }).catch(error => {\n        console.error('Erro ao processar a requisição:', error);\n      });\n    },\n    handleFileInputChange(event) {\n      this.selectedFile = event.target.files[0];\n      this.imageUrl = URL.createObjectURL(this.selectedFile);\n    },\n    // Função para enviar a imagem para o backend\n    enviarImagemParaBackend(selectedFile, produto) {\n      // Cria um objeto FormData\n      let formData = new FormData();\n      // Adiciona a imagem ao objeto FormData\n      formData.append('imagem', selectedFile);\n      enviarImagem(formData, produto.id).then(response => {\n        console.log('Imagem enviada com sucesso para o backend:', response);\n        // Faça algo com a resposta, se necessário\n      }).catch(error => {\n        console.error('Erro ao enviar imagem para o backend:', error);\n        // Faça algo com o erro, se necessário\n      });\n    },\n    uploadImage() {\n      if (!this.selectedFile) {\n        alert('Por favor, selecione um arquivo.');\n        return;\n      }\n    }\n  },\n  mounted() {\n    console.log(this.$route.params.id);\n  }\n};","map":{"version":3,"mappings":"AAoBA,SAAQA,QAAQ,EAAEC,YAAY,QAAO,+BAA8B;AAEnE,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,UAAU,EAAE,CACZ,CAAC;EACDC,IAAIA,GAAG;IACD,OAAO;MACHC,WAAW,EAAE,IAAI;MACjBC,YAAY,EAAE,IAAI;MAClBC,aAAa,EAAE,EAAE;MACjBC,wBAAwB,EAAE;QACtBA,wBAAwB,EAAE,IAAI;QAC9BC,YAAY,EAAE,IAAI;QAClBC,mBAAmB,EAAE,IAAI;QACzBC,mBAAmB,EAAE;MACzB,CAAC;MACDC,eAAe,EAAE,EAAE;MACnBC,SAAS,EAAE,CAAC,CAAC;MACbC,MAAM,EAAE;QACJC,IAAI,EAAE,CAAC;QACPC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,kBAAkB;QACxBC,GAAG,EAAE;MACT,CAAC;MACDC,MAAM,EAAE;QACJC,cAAc,EAAE,EAAE;QAClBC,UAAU,EAAE,EAAE;QACdC,QAAQ,EAAE,EAAE;QACZC,aAAa,EAAE,EAAE;QACjBC,YAAY,EAAE;MAClB,CAAC;MACDC,uBAAuB,EAAE,IAAI;MAC7BC,yBAAyB,EAAE,KAAK;MAChCC,YAAY,EAAE,IAAI;MAClBC,QAAQ,EAAE;IACd,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,GAAE;MAET,MAAMC,KAAI,GAAI;QACZ,aAAa,EAAE,IAAI,CAAC1B,WAAW;QAC/B,cAAc,EAAE,IAAI,CAACC;MACvB,CAAC;MAGDN,QAAQ,CAAC;QACDK,WAAW,EAAE,IAAI,CAACA,WAAW;QAC7BC,YAAY,EAAE,IAAI,CAACA;MACvB,CAAC,CAAC,CAAC0B,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAAEF,QAAQ,CAAC;QAC7C,MAAMG,OAAM,GAAIH,QAAQ;QAExB,IAAI,CAACI,uBAAuB,CAAC,IAAI,CAACV,YAAY,EAAES,OAAO,CAAC;MAE5D,CAAC,CAAC,CAACE,KAAK,CAACC,KAAI,IAAK;QACdL,OAAO,CAACK,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MAC3D,CAAC,CAAC;IAER,CAAC;IAEDC,qBAAqBA,CAACC,KAAK,EAAE;MAC3B,IAAI,CAACd,YAAW,GAAIc,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MACzC,IAAI,CAACf,QAAO,GAAIgB,GAAG,CAACC,eAAe,CAAC,IAAI,CAAClB,YAAY,CAAC;IACxD,CAAC;IAED;IACAU,uBAAuBA,CAACV,YAAY,EAAES,OAAO,EAAC;MAC1C;MACA,IAAIU,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC7B;MACAD,Q