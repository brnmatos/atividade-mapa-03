{"ast":null,"code":"import TabelaProdutos from '@/components/TabelaProdutos';\nimport { filtrarProdutos } from '@/services/ComprarService.js';\nexport default {\n  name: 'Produtos',\n  components: {\n    TabelaProdutos\n  },\n  data() {\n    return {\n      produtos: {},\n      paginacao: {\n        pagina: 0,\n        ordenacao: 'id',\n        tamanho: 10,\n        total: 0\n      }\n    };\n  },\n  computed: {\n    qtdContratos() {\n      return this.paginacao.total === 1 ? '1 produto' : `${this.paginacao.total} produtos`;\n    }\n  },\n  mounted() {\n    this.filtrarProdutosView();\n  },\n  methods: {\n    async filtrarProdutosView() {\n      try {\n        const response = await filtrarProdutos({\n          pagina: this.paginacao.pagina + 1,\n          ordenacao: this.paginacao.ordenacao,\n          tamanho: this.paginacao.tamanho\n        });\n\n        // Verifique se `response.data.itens` existe e é um array\n        if (Array.isArray(response.data.itens)) {\n          // Atribua diretamente o array `response.data.itens` à variável `produtos`\n          this.produtos = response.data.itens;\n        } else {\n          console.error('Itens não é um array:', response.data.itens);\n        }\n        console.log('produto:', this.produtos);\n        this.paginacao.total = response.data.paginacao.total;\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    atualizarPagina(pagina) {\n      this.paginacao.pagina = pagina;\n      this.filtrarProdutos();\n    },\n    mudarPagina(novaPagina) {\n      this.paginacao.pagina = novaPagina;\n      this.params.page = novaPagina;\n      this.filtrarProdutos();\n    }\n  }\n};","map":{"version":3,"mappings":"AAYA,OAAOA,cAAa,MAAO,6BAA6B;AACxD,SAAQC,eAAe,QAAO,8BAA8B;AAE5D,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVH;EACF,CAAC;EAEDI,IAAIA,GAAE;IACJ,OAAO;MACLC,QAAQ,EAAE,CAAC,CAAC;MACZC,SAAS,EAAE;QACDC,MAAM,EAAE,CAAC;QACTC,SAAS,EAAE,IAAI;QACfC,OAAO,EAAE,EAAE;QACXC,KAAK,EAAE;MACX;IACN;EAEJ,CAAC;EAEDC,QAAQ