{"ast":null,"code":"import TabelaProdutos from '@/components/TabelaProdutos';\nimport { filtrarProdutos } from '@/services/ComprarService.js';\nexport default {\n  name: 'Produtos',\n  components: {\n    TabelaProdutos\n  },\n  data() {\n    return {\n      produtos: [],\n      paginacao: {\n        pagina: 0,\n        ordenacao: 'id',\n        tamanho: 10,\n        total: 0\n      }\n    };\n  },\n  computed: {\n    qtdContratos() {\n      return this.paginacao.total === 1 ? '1 produto' : `${this.paginacao.total} produtos`;\n    }\n  },\n  mounted() {\n    this.filtrarProdutos();\n  },\n  methods: {\n    async filtrarProdutos() {\n      console.log('enter here....');\n      try {\n        const {\n          data\n        } = await filtrarProdutos(this.paginacao);\n        this.produtos = data.itens;\n        this.paginacao.total = data.paginacao.total;\n      } catch (error) {\n        this.$notify.error({\n          message: 'Erro na requisição de buscar produtos.'\n        });\n      }\n    },\n    atualizarPagina(pagina) {\n      this.paginacao.pagina = pagina;\n      this.filtrarProdutos();\n    }\n  }\n};","map":{"version":3,"mappings":"AAwBA,OAAOA,cAAa,MAAO,6BAA6B;AACxD,SAAQC,eAAe,QAAO,8BAA8B;AAE5D,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVH;EACF,CAAC;EAEDI,IAAIA,GAAE;IACJ,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE;QACDC,MAAM,EAAE,CAAC;QACTC,SAAS,EAAE,IAAI;QACfC,OAAO,EAAE,EAAE;QACXC,KAAK,EAAE;MACX;IACN;EAEJ,CAAC;EAEDC,QAAQ,EAAE;IACJC,YAAYA,GAAG;MACX,OAAO,IAAI,CAACN,SAAS,CAACI,KAAI,KAAM,IAC1B,WAAU,GACT,GAAE,IAAI,CAACJ,SAAS,CAACI,KAAK,WAAW;IAC5C;EACJ,CAAC;EAEHG,OAAOA,GAAE;IACP,IAAI,CAACZ,eAAe,CAAC,CAAC;EACxB,CAAC;EAEDa,OAAO,EAAE;IAEP,MAAMb,eAAeA,GAAE;MACjBc,OAAO,CAACC,GAAG,CAAC,gBAAgB,CAAC;MAC3B,IAAI;QACA,MAAM;UAAEZ;QAAK,IACT,MAAMH,eAAe,CACjB,IAAI,CAACK,SACT,CAAC;QACL,IAAI,CAACD,QAAO,GAAID,IAAI,CAACa,KAAK;QAC1B,IAAI,CAACX,SAAS,CAACI,KAAI,GAAIN,IAAI,CAACE,SAAS,CAACI,KAAK;MAC/C,EAAE,OAAOQ,KAAK,EAAE;QACZ,IAAI,CAACC,OAAO,CAACD,KAAK,CAAC;UACfE,OAAO,EACH;QACR,CAA