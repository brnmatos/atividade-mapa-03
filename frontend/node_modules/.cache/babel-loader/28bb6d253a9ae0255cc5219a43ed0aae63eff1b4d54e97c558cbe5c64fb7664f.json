{"ast":null,"code":"import AudioPlayer from '@liripeng/vue-audio-player';\nimport MarqueeText from 'vue-marquee-text-component';\nexport default {\n  name: \"Navbar\",\n  props: [\"logo\", \"alt\"],\n  components: {\n    AudioPlayer,\n    MarqueeText\n  },\n  data() {\n    return {\n      currentAudioName: '',\n      audioList: [{\n        name: 'Alessandro Vilas Boas - O Carpinteiro',\n        url: 'https://www.soundhelix.com/examples/mp3/SoundHelix-Song-1.mp3'\n      }, {\n        name: 'audio2',\n        url: 'https://www.soundhelix.com/examples/mp3/SoundHelix-Song-8.mp3'\n      }]\n    };\n  },\n  methods: {\n    // Something to do before playing\n    handleBeforePlay(next) {\n      // There are a few things you can do here...\n      this.currentAudioName = this.audioList[this.$refs.audioPlayer.currentPlayIndex].name;\n      next(); // Start playing\n    },\n\n    pegaNomeMusicaStart() {\n      this.$nextTick(() => {\n        this.currentAudioName = this.audioList[0].name;\n        this.$refs.audioPlayer.play();\n      });\n    }\n  },\n  mounted() {\n    this.pegaNomeMusicaStart();\n  }\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,WAAU,MAAO,4BAA2B;AACnD,OAAOC,WAAU,MAAO,4BAA2B;AAEnD,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,KAAK,EAAE,CAAC,MAAM,EAAE,KAAK,CAAC;EAEtBC,UAAU,EAAE;IACVJ,WAAW;IACXC;EACF,CAAC;EAEDI,IAAI,GAAG;IACL,OAAO;MACLC,gBAAgB,EAAE,EAAE;MACpBC,SAAS,EAAE,CACT;QACEL,IAAI,EAAE,uCAAuC;QAC7CM,GAAG,EAAE;MACP,CAAC,EACD;QACEN,IAAI,EAAE,QAAQ;QACdM,GAAG,EAAE;MACP;IAEJ;EACF,CAAC;EAEDC,OAAO,EAAE;IACP;IACAC,gBAAgB,CAACC,IAAI,EAAE;MACrB;MACA,IAAI,CAACL,gBAAe,GAAI,IAAI,CAACC,SAAS,CAAC,IAAI,CAACK,KAAK,CAACC,WAAW,CAACC,gBAAgB,CAAC,CAACZ,IAAG;MAEnFS,IAAI,EAAC,EAAE;IACT,CAAC;;IAEDI,mBAAmB,GAAE;MACnB,IAAI,CAACC,SAAS,CAAC,MAAM;QACnB,IAAI,CAACV,gBAAe,GAAI,IAAI,CAACC,SAAS,CAAC,CAAC,CAAC,CAACL,IAAI;QAC9C,IAAI,CAACU,KAAK,CAACC,WAAW,CAACI,IAAI,EAAE;MAC/B,CAAC,CAAC;IAEJ;EACF,CAAC;EAEDC,OAAO,GAAG;IACR,IAAI,CAACH,mBAAmB,EAAE;EAC5B;AAEF","names":["AudioPlayer","MarqueeText","name","props","components","data","currentAudioName","audioList","url","methods","handleBeforePlay","next","$refs","audioPlayer","currentPlayIndex","pegaNomeMusicaStart","$nextTick","play","mounted"],"sourceRoot":"","sources":["C:\\app\\137-tech\\radio-web\\radio-web\\frontend\\src\\components\\Navbar.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div id=\"nav\">\r\n      <router-link id=\"logo-url\" to=\"/\">\r\n        <img id=\"logo\" :src=\"logo\" :alt=\"alt\">\r\n      </router-link>\r\n        <audio-player\r\n          class=\"audioPlayer\"\r\n          id=\"audioPlayer\"\r\n          ref=\"audioPlayer\"\r\n          :audio-list=\"audioList.map(elm => elm.url)\"\r\n          :before-play=\"handleBeforePlay\"\r\n          :show-prev-button=\"false\"\r\n          :show-next-button=\"false\"\r\n          :show-progress-bar=\"false\"\r\n          :show-playback-rate=\"false\"\r\n          :show-volume-button=\"false\"\r\n          theme-color=\"#fff\"\r\n        />\r\n        <marquee-text class=\"nomeMusica\" :duration=\"6\" :repeat=\"1\">\r\n          {{ currentAudioName }}\r\n        </marquee-text>          \r\n      <router-link to=\"/\">Página Inicial</router-link>\r\n      <router-link to=\"/programacao\">Programação</router-link>\r\n      <router-link to=\"/contato\">Contato</router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AudioPlayer from '@liripeng/vue-audio-player'\r\nimport MarqueeText from 'vue-marquee-text-component'\r\n\r\nexport default {\r\n  name: \"Navbar\",\r\n  props: [\"logo\", \"alt\"],\r\n  \r\n  components: {\r\n    AudioPlayer,\r\n    MarqueeText\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      currentAudioName: '',\r\n      audioList: [\r\n        {\r\n          name: 'Alessandro Vilas Boas - O Carpinteiro',\r\n          url: 'https://www.soundhelix.com/examples/mp3/SoundHelix-Song-1.mp3'\r\n        },\r\n        {\r\n          name: 'audio2',\r\n          url: 'https://www.soundhelix.com/examples/mp3/SoundHelix-Song-8.mp3'\r\n        }\r\n      ]\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    // Something to do before playing\r\n    handleBeforePlay(next) {\r\n      // There are a few things you can do here...\r\n      this.currentAudioName = this.audioList[this.$refs.audioPlayer.currentPlayIndex].name\r\n\r\n      next() // Start playing\r\n    },\r\n    \r\n    pegaNomeMusicaStart(){\r\n      this.$nextTick(() => {\r\n        this.currentAudioName = this.audioList[0].name;\r\n        this.$refs.audioPlayer.play();\r\n      });\r\n      \r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.pegaNomeMusicaStart();\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  #nav {\r\n    background-color: #222;\r\n    border-bottom: 4px solid #111;\r\n    padding: 15px 50px;\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    align-items: center;\r\n  }\r\n  #nav {\r\n    margin: auto;\r\n    margin-left: 0;\r\n  }\r\n  #logo-url {\r\n    margin: auto !important;\r\n    margin-left: 0 !important;\r\n    margin-right: 0 !important;\r\n  }\r\n\r\n  .audioPlayer{\r\n    margin: auto !important;\r\n    margin-left: 0 !important;\r\n  }\r\n\r\n  #logo {\r\n    width: 60px;\r\n    height: 60px;\r\n  }\r\n  #nav a {\r\n    color: #FFF;\r\n    text-decoration: none;\r\n    margin: 12px;\r\n    transition: .5s;\r\n  }\r\n  #nav a:hover {\r\n    color: #FFF;\r\n  }\r\n\r\n  .nomeMusica{\r\n    color: #FFF;\r\n    text-decoration: none;\r\n    transition: .5s;\r\n    margin: auto !important;\r\n    margin-left: 10px !important;\r\n  }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}