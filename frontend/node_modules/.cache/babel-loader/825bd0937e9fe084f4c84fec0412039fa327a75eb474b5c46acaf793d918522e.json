{"ast":null,"code":"import TabelaProdutos from '@/components/TabelaProdutos';\nimport { filtrarProdutos } from '@/services/ComprarService.js';\nexport default {\n  name: 'Produtos',\n  components: {\n    TabelaProdutos\n  },\n  data() {\n    return {\n      produtos: [],\n      paginacao: {\n        pagina: 0,\n        ordenacao: 'id',\n        tamanho: 10,\n        total: 0\n      }\n    };\n  },\n  computed: {\n    qtdContratos() {\n      return this.paginacao.total === 1 ? '1 produto' : `${this.paginacao.total} produtos`;\n    }\n  },\n  mounted() {\n    this.filtrarProdutosView();\n  },\n  methods: {\n    async filtrarProdutosView() {\n      try {\n        const response = await filtrarProdutos({\n          pagina: this.paginacao.pagina + 1,\n          ordenacao: this.paginacao.ordenacao,\n          tamanho: this.paginacao.tamanho\n        });\n        this.produtos = response.data.itens;\n        this.paginacao.total = response.data.paginacao.total;\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    atualizarPagina(pagina) {\n      this.paginacao.pagina = pagina;\n      this.filtrarProdutos();\n    }\n  }\n};","map":{"version":3,"mappings":"AAwBA,OAAOA,cAAa,MAAO,6BAA6B;AACxD,SAAQC,eAAe,QAAO,8BAA8B;AAE5D,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVH;EACF,CAAC;EAEDI,IAAIA,GAAE;IACJ,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE;QACDC,MAAM,EAAE,CAAC;QACTC,SAAS,EAAE,IAAI;QACfC,OAAO,EAAE,EAAE;QACXC,KAAK,EAAE;MACX;IACN;EAEJ,CAAC;EAEDC,QAAQ,EAAE;IACJC,YAAYA,GAAG;MACX,OAAO,IAAI,CAACN,SAAS,CAACI,KAAI,KAAM,IAC1B,WAAU,GACT,GAAE,IAAI,CAACJ,SAAS,CAACI,KAAK,WAAW;IAC5C;EACJ,CAAC;EAEHG,OAAOA,GAAE;IACP,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC5B,CAAC;EAEDC,OAAO,EAAE;IAEP,MAAMD,mBAAmBA,GAAG;MAC1B,IAAI;QACF,MAAME,QAAO,GAAI,MAAMf,eAAe,CAAC;UACrCM,MAAM,EAAE,IAAI,CAACD,SAAS,CAACC,MAAK,GAAI,CAAC;UACjCC,SAAS,EAAE,IAAI,CAACF,SAAS,CAACE,SAAS;UACnCC,OAAO,EAAE,IAAI,CAACH,SAAS,CAACG;QAC1B,CAAC,CAAC;QACF,IAAI,CAACJ,QAAO,GAAIW,QAAQ,CAACZ,IAAI,CAACa,KAAK;QACnC,IAAI,CAACX,SAAS,C